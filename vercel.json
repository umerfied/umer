{
  "version": 2,
  "builds": [
    {
      "src": "umer/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static",
    	"config":{"distDir":"staticfiles"}
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/website/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/umer/wsgi.py"
    }
  ]
}

